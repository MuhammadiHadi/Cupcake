<?xml version="1.0" encoding="utf-8"?>
<resources>



    <style name="textInputLayoutStyle"
        parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxCornerRadiusBottomEnd">13dp</item>
        <item name="boxCornerRadiusBottomStart">13dp</item>
        <item name="boxCornerRadiusTopEnd">13dp</item>
        <item name="boxCornerRadiusTopStart">13dp</item>
        <item name="boxStrokeColor">@color/black</item>
        <item name="boxStrokeWidth">0.6dp</item>
        <item name="hintAnimationEnabled">true</item>
        <item name="expandedHintEnabled">false</item>

    </style>
    <style name="edtTvText16RobotoBlackPearl">
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:inputType">text</item>
        <item name="android:imeOptions">actionNext</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="textInputLayoutStyleHintEnable"
        parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxCornerRadiusBottomEnd">13dp</item>
        <item name="boxCornerRadiusBottomStart">13dp</item>
        <item name="boxCornerRadiusTopEnd">13dp</item>
        <item name="boxCornerRadiusTopStart">13dp</item>
        <item name="boxStrokeWidth">0.6dp</item>
        <item name="hintAnimationEnabled">true</item>
        <item name="expandedHintEnabled">true</item>

    </style>
    //tv mean textView 20 mean text size RobotoBold fontsFamily and cetaceanBlue  is a Color name
    <style name="tv20RobotoBoldCetaceanBlue">

    </style>
</resources>